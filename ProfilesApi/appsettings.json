      { "Name": "Console" }
    ]
  },
  "ConnectionStrings": {
  },
  "MongoDBSettings": {
    //"MongoDbProfilesApi": "mongodb://localhost:27017/",
    "MongoDbProfilesApi": "mongodb://mongodb:27017/",
    "DatabaseName": "ProfilesApi"
  },
  "AllowedHosts": "*"
}
